<access origin="http://thenutspokerleague.com" subdomains="true" />
<access origin="http://google.com" subdomains="true" />
<access origin="http://googleapis.com" subdomains="true" />
<access origin="http://primointeractive.com" subdomains="true" />
<access origin="http://guaranteed-carloans.co.uk" subdomains="true" />
